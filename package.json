{
  "name": "eslint-plugin-ember-best-practices",
  "version": "1.1.1",
  "description": "Eslint rules for linting for anti-patterns in Ember applications.",
  "main": "lib/index.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc mocha --recursive tests",
    "test-server": "mocha --recursive --growl --watch tests",
    "generate-authors": "git shortlog -sne | cut -f 2 | sort | uniq > AUTHORS.txt"
  },
  "repository": "ember-best-practices/eslint-plugin-ember-best-practices",
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "devDependencies": {
    "coveralls": "^2.13.1",
    "eslint": "^4.11.0",
    "mocha": "^3.2.0",
    "mocha-eslint": "^3.0.1",
    "nyc": "^11.2.1"
  },
  "dependencies": {
    "requireindex": "^1.1.0"
  }
}
